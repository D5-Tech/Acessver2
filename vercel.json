    { "src": "/jarvis2025 ver 1/index.html", "use": "@vercel/static" },
    { "src": "api/webhook.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/webhook", "dest": "/api/webhook.js" },
    { "src": "/api/webhook", "dest": "/api/webhook.js" },
