{
  "version": 2,
  "builds": [
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "main.html", "use": "@vercel/static" },
    { "src": "api/webhook.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/webhook", "dest": "/api/webhook.js" },
    { "src": "/", "dest": "/index.html" },
    { "src": "/main", "dest": "/main.html" }
  ]
}