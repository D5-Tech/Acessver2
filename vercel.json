    { "src": "/jarvis2025%20ver%201/index.html", "dest": "/jarvis2025 ver 1/index.html" },
    { "src": "/jarvis2025 ver 1/index.html", "dest": "/jarvis2025 ver 1/index.html" }
  ]  
}